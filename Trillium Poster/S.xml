    <key>SMLC-1</key>
       <dict>
          <key>elementName</key>
          <string>Serving Mobile Location Center (SMLC)</string>
          <key>network</key>
          <string>WCDMA</string>
          <key>educationalDescription</key>
          <string>
             <![CDATA[
            <p>
            The SMLC is responsible for overall coordination and scheduling of resources that are necessary to 
